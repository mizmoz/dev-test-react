{
	"name": "karmarama",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test --env=jsdom",
		"eject": "react-app-rewired eject"
	},
	"dependencies": {
		"polished": "^2.3.3",
		"prop-types": "^15.6.2",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-scripts": "2.1.3",
		"styled-components": "^4.1.3"
	},
	"devDependencies": {
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-react": "^7.12.4",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.3",
		"prettier": "^1.16.4",
		"react-app-rewired": "^2.0.3"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		],
		"*.js": [
			"eslint --fix",
			"git add",
			"jest --bail --findRelatedTests"
		]
	}
}
